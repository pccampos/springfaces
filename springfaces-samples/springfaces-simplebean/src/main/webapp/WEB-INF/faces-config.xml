<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<navigation-rule>
		<from-view-id>/WEB-INF/pages/navigation/rulelink.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>rulelink</from-outcome>
			<to-view-id>/spring:@destination</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/WEB-INF/pages/navigation/rulebutton.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>rulebutton</from-outcome>
			<to-view-id>/spring:@destination</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/WEB-INF/pages/navigation/rulecommandlink.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>rulecommandlink</from-outcome>
			<to-view-id>/spring:@destination</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/WEB-INF/pages/navigation/rulecommandbutton.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>rulecommandbutton</from-outcome>
			<to-view-id>/spring:@destination</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
